pipeline {
    agent any
 tools {
        maven 'Maven 3.6.0'
        jdk 'jdk11'
    }

    stages {
        stage('Build') {
            steps {
		cd myliquibase
                bat "mvn install"
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
               echo 'Deployment..'
            }
        }
    }
}